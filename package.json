{
  "name": "quickp2p-demo",
  "version": "1.0.0",
  "dependencies": {
    "axios": "^1.6.5",
    "eventemitter3": "^5.0.1",
    "query-string": "^8.1.0",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-scripts": "^5.0.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-lib": "cross-env NODE_ENV=production babel src/lib --out-dir build/lib --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm start"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "license": "MIT"
}
